{
  "name": "pixi-dragonbones-runtime",
  "version": "8.0.0",
  "type": "module",
  "author": "h1ve2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h1ve2/pixi-dragonbones-runtime.git"
  },
  "keywords": [
    "dragonbones",
    "pixi.js",
    "pixiv8",
    "armature",
    "animate"
  ],
  "description": "DragonBones Runtime for Pixi.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "start": "npm run clean && tsc --watch",
    "clean": "rimraf ./lib",
    "prepublishOnly": "npm run build",
    "publish-ci": "npm publish"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "lib/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "pixi.js": "^8.0.0"
  },
  "devDependencies": {
    "install-peers": "^1.0.4",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3"
  }
}
