{
  "name": "@h1ve2/pixi-dragonbones-runtime-docs",
  "version": "0.0.0",
  "main": "index.js",
  "author": "h1ve2",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "typedoc": "^0.28.1",
    "typedoc-plugin-markdown": "^4.6.1",
    "vitepress": "^1.6.3",
    "typescript": "^5.6.3",
    "rimraf": "^6.0.1"
  },
  "scripts": {
    "docs:dev": "vitepress dev",
    "docs:build": "npm run typedoc:all && vitepress build",
    "docs:preview": "vitepress preview",
    "clean": "rimraf ./api",
    "typedoc:md": "typedoc --lang zh --cleanOutputDir --out ./api/8.x/",
    "typedoc:json": "typedoc --json ./api/8.x/doc.json  --cleanOutputDir false",
    "typedoc:all": "npm run typedoc:md && npm run typedoc:json"
  },
  "dependencies": {
    "vitepress-plugin-group-icons": "^1.3.8"
  }
}
